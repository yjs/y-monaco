{
  "name": "y-monaco",
  "version": "0.1.1",
  "description": "Monaco editor bindings for Yjs",
  "main": "./dist/y-monaco.js",
  "module": "./src/y-monaco.js",
  "sideEffects": false,
  "scripts": {
    "start": "npm run demo",
    "demo": "webpack-dev-server --open-page .",
    "dist": "npm run clean && rollup -c",
    "test": "rollup -c && node test/test.node.js",
    "lint": "standard",
    "watch": "rollup -wc",
    "debug": "concurrently 'live-server --port=3443 --entry-file=test.html' 'npm run watch'",
    "preversion": "npm run lint && npm run dist && npm run test",
    "clean": "rm -rf dist demo/dist"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y-js/y-monaco.git"
  },
  "keywords": [
    "Yjs",
    "Monaco"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/y-js/y-monaco/issues"
  },
  "homepage": "https://github.com/y-js/y-monaco#readme",
  "dependencies": {
    "lib0": "^0.2.35"
  },
  "peerDependencies": {
    "yjs": "^13.3.1 && >=13.3.1",
    "y-protocols": ">=1.0.1",
    "monaco-editor": ">=0.20.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/node": "^12.19.12",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "file-loader": "^6.2.0",
    "jsdom": "^15.2.1",
    "live-server": "^1.2.1",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.1.0",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^3.1.8",
    "standard": "^14.3.4",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1",
    "y-protocols": "^1.0.2",
    "y-websocket": "^1.3.9",
    "yjs": "^13.4.9"
  }
}
